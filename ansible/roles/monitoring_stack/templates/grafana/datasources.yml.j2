apiVersion: 1
datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    url: http://localhost:{{ prometheus_port }}
    isDefault: true
    editable: false
    jsonData:
      httpMethod: GET
      timeInterval: "15s"
  - name: Alertmanager
    type: alertmanager
    access: proxy
    url: http://localhost:{{ alertmanager_port }}
    jsonData:
      implementation: "prometheus"